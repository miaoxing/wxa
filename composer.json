{
  "name": "miaoxing/wechat-mp",
  "description": "Miaoxing WeChat Mini Program Plugin",
  "keywords": [
    "miaoxing", "wechat", "mini program"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatMp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatMp\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
